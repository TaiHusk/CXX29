/** @file experimental/smart_view
 *  This is a TS C++ Library header.
*/

#ifndef _GLIBCXX_EXPERIMENTAL_SMART_VIEW
#define _GLIBCXX_EXPERIMENTAL_SMART_VIEW 1

#pragma GCC system_header

#include <c++/29/experimental/smart_view_string>
#include <c++/29/experimental/smart_view_vector>

#include <bits/c++config.h>

namespace std _GLIBCXX_VISIBILITY(default) 
{
_GLIBCXX_BEGIN_NAMESPACE_VERSION

    namespace smart_view {
        namespace string { using namespace smart_view_string; }
        namespace vector { using namespace smart_view_vector; }
    }

_GLIBCXX_END_NAMESPACE_VERSION
} // namespace

#endif /* _GLIBCXX_EXPERIMENTAL_SMART_VIEW */
